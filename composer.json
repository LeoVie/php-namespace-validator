{
  "name": "leovie/php-nf",
  "type": "library",
  "description": "Validates php namespaces",
  "keywords": ["namespace"],
  "homepage": "https://github.com/LeoVie/php-nf",
  "license": "MIT",
  "authors": [
    {
      "name": "Leo Viezens",
      "role": "Developer"
    }
  ],
  "require": {
    "symfony/console": "^4.2",
    "symfony/finder": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/http-kernel": "^4.2",
    "symfony/config": "^4.2",
    "symfony/yaml": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "LeoVie\\PhpNf\\": "src/Classes/"
    },
    "classmap": ["src/AppKernel.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "LeoVie\\PhpNf\\Tests\\": "src/Tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "infection/infection": "^0.13.1",
    "mockery/mockery": "^1.2"
  },
  "config": {
    "bin-dir": "bin",
    "optimize-autoloader": true
  },
  "bin": [
    "bin/php-nf"
  ]
}
