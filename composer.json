{
  "name": "leovie/php-namespace-validator",
  "type": "library",
  "description": "Validates php namespaces",
  "keywords": ["namespace"],
  "homepage": "https://github.com/LeoVie/php-namespace-validator",
  "license": "MIT",
  "authors": [
    {
      "name": "Leo Viezens",
      "role": "Developer"
    }
  ],
  "require": {
    "symfony/console": "^4.2",
    "symfony/finder": "^4.2",
    "symfony/dependency-injection": "^4.2",
    "symfony/http-kernel": "^4.2",
    "symfony/config": "^4.2",
    "symfony/yaml": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "LeoVie\\": "src/Classes/"
    },
    "classmap": ["src/AppKernel.php"]
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "config": {
    "bin-dir": "bin",
    "optimize-autoloader": true
  },
  "bin": [
    "bin/php-namespace-validator"
  ]
}
